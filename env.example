# Solace Configuration (for local testing)
SOLACE_DEV_MODE=true
SOLACE_BROKER_URL=ws://localhost:8080
SOLACE_BROKER_USERNAME=default
SOLACE_BROKER_PASSWORD=default
SOLACE_BROKER_VPN=default

# LLM Configuration (using OpenAI for testing)
LLM_SERVICE_ENDPOINT=https://api.openai.com/v1
LLM_SERVICE_API_KEY=your-openai-api-key-here
LLM_SERVICE_PLANNING_MODEL_NAME=gpt-3.5-turbo
LLM_SERVICE_GENERAL_MODEL_NAME=gpt-3.5-turbo
LLM_REPORT_MODEL_NAME=gpt-3.5-turbo

# Image Generation (using OpenAI DALL-E for testing)
IMAGE_SERVICE_ENDPOINT=https://api.openai.com/v1
IMAGE_SERVICE_API_KEY=your-openai-api-key-here
IMAGE_MODEL_NAME=dall-e-3

# SAM Configuration
NAMESPACE=cloutprint
SESSION_SECRET_KEY=your-secret-key-here
WEBUI_GATEWAY_ID=webui-gateway
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
ENABLE_EMBED_RESOLUTION=true
GATEWAY_ARTIFACT_LIMIT_BYTES=10000000
SSE_MAX_QUEUE_SIZE=200
